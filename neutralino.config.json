{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/frontend/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "myneuapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/frontend/public/favicon.ico"
    }
  },
  "cli": {
    "binaryName": "myneuapp",
    "resourcesPath": "/frontend/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/frontend/public/neutralino.js",
    "binaryVersion": "4.9.0",
    "clientVersion": "3.8.0"
  }
}